{
  "name": "vivares-design-tokens",
  "version": "0.2.4",
  "description": "Tokens base architecture",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "devDependencies": {
    "optimist": "^0.6.1",
    "theo": "^8.1.5"
  },
  "scripts": {
    "web": "theo src/tokens.yml --transform web --format scss --dest dist",
    "web:styl": "theo src/tokens.yml --transform web --format styl --dest dist",
    "web:module": "theo src/tokens.yml --transform web --format module.js --dest dist",
    "web:html": "theo src/tokens.yml --transform web --format html --dest dist"
  },
  "keywords": [
    "tokens",
    "design tokens"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeurgo/design-tokens"
  }
}
